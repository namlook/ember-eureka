{{#if field.values}}
<ul>
    {{#if field.values.isPending}}
        <li>loading...</li>
    {{else}}
        {{#each relation in field.values}}
            <li>{{#link-to relation.meta.modelIndexViewPath relation._id}}{{relation.title}} ({{relation._id}}){{/link-to}}</li>
        {{/each}}
    {{/if}}
</ul>
{{/if}}