
{{#if isText}}
    {{input type='text' value=value}}
{{else}} {{#if isBoolean}}
    {{input type='checkbox' checked=value}}
{{else}} {{#if isNumber}}
    {{input type='number' value=value}}
{{else}} {{#if isDate}}
    {{#date-picker value=value placeholder="select a date&hellip;"}}{{/date-picker}}
{{else}}
    {{input type=inputType value=value}}
{{/if}} {{/if}} {{/if}} {{/if}}
