
{{#if isText}}
    {{input type='text' value=value name=fieldName autocomplete="off" class="eureka-field-input form-control"}}
{{else}} {{#if isBoolean}}
    {{input type='checkbox' checked=value name=fieldName autocomplete="off" class="eureka-field-input form-control"}}
{{else}} {{#if isNumber}}
    {{input type='number' value=value name=fieldName autocomplete="off" class="eureka-field-input form-control"}}
{{else}} {{#if isDate}}
    {{#date-picker value=value name=fieldName autocomplete="off" class="eureka-field-input form-control"}}{{/date-picker}}
{{else}}
    {{input type=inputType value=value name=fieldName autocomplete="off" class="eureka-field-input form-control"}}
{{/if}} {{/if}} {{/if}} {{/if}}
