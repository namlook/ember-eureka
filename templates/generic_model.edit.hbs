

<h1>{{{model.__title__}}}</h1>

<form class="form-horizontal" role="form">
    {{model-form model=model}}

     <div class="form-group">
        <div class="col-sm-offset-4 col-sm-8">
            {{#link-to model.__meta__.displayRouteName model._id
                classNames="btn btn-default cancel eureka-link-to-display"
                classNameBindings="model.__meta__.EurekaGenericModelModelCSS"}}cancel{{/link-to}}
            <button class="btn btn-primary save" {{action 'save'}}>save</button>
        </div>
    </div>
</form>

