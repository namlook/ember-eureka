
<h1 class="eureka-new-document-title">New {{model.__meta__.type}}</h1>

<form class="form-horizontal eureka-new-document-form" role="form">
    {{model-form model=model}}

     <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            {{#model-to 'list' model classNames="btn btn-default cancel"}}cancel{{/model-to}}
            <button {{bind-attr class=":btn :btn-primary :eureka-save-action model.__meta__.dasherizedType"}} {{action 'save'}}>save</button>
        </div>
    </div>
</form>