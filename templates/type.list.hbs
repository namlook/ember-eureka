
{{#each item in model}}
    <fieldset>
        <legend>{{model.type}}</legend>
        <h2>{{#link-to 'type.display' item._type item._id}}{{item.title}}{{/link-to}}</h2>
        {{#if item.description}}
            <p>{{item.description}}</p>
        {{/if}}
        {{#if item.thumb}}
            <img {{bind-attr src=item.thumb}} />
        {{/if}}
    </fieldset>
{{/each}}